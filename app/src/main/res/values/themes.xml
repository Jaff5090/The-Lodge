<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppNameWhite" parent="Theme.MaterialComponents.DayNight.NoActionBar.Bridge">
        <!-- Primary brand color. -->
        <item name="android:fontFamily">@font/roboto_italic</item>
        <item name="colorPrimary">#000000</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/white</item>



        <!-- Customize your theme here. -->



    </style>
    <style name="MyCustomDialogStyle" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:background">#020202</item>
        <item name="colorAccent">#456CFA</item>
        <item name="android:textColorPrimary">@color/gray</item>
        <item name="android:textColorSecondary">#000000</item>
    </style>



</resources>